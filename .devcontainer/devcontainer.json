{
	"name": "gem5-sandbox",
	"dockerComposeFile": "docker-compose.yml",
	"service": "development",
  	"workspaceFolder": "/workspace",
	"remoteUser": "default",
	"settings": { 
		"terminal.integrated.shell.linux": "/bin/bash",
		"python.pythonPath": "/usr/local/bin/python",
		"python.linting.enabled": false
	},
	"extensions": [
		"ms-python.python",
		"ms-azuretools.vscode-docker",
		"mauve.terraform"
	]
}